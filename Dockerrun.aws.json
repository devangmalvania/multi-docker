{
    "AWSEBDockerrunversion": 2,
    "containerDefinitions": [
        {
            "name" : "client",
            "image": "dmalvania/multi-client",
            "hostname": "client",
            "essential": false
        },
        {
            "name" : "server",
            "image": "dmalvania/multi-server",
            "hostname": "api",
            "essential": false
        },
        {
            "name" : "worker",
            "image": "dmalvania/multi-worker",
            "hostname": "worker",
            "essential": false
        },
        {
            "name" : "nginx",
            "image": "dmalvania/multi-nginx",
            "hostname": "nginx",
            "essential": true,
            "portMappings" : [
                {
                    "hostPort" : 80,
                    "containerPort" : 80
                }
            ],
            "links":["client","server"]

        }

        
    ]
}